  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings",
    "deploy-sand": "git checkout sandbox; git push -u origin sandbox",
    "merge": "git checkout master; git merge sandbox",
    "deploy": "git checkout master; git push -u origin master",
    "directive": "cd src/system_templates/directives && cp directive.ts new.ts; mv new.ts ../../../src/ts/modules/directives; cd ../../../src/ts/modules/directives; mv new.ts ",
    "service": "cd src/system_templates/services && cp service.ts new.ts; mv new.ts ../../../src/ts/modules/services; cd ../../../src/ts/modules/services; mv new.ts ",
    "pipe": "cd src/system_templates/pipes && cp pipe.ts new.ts; mv new.ts ../../../src/ts/modules/pipes; cd ../../../src/ts/modules/pipes; mv new.ts ",
    "component": "cd src/system_templates/components && cp component.ts new.ts; mv new.ts ../../../src/ts/modules/components; cd ../../../src/ts/modules/components; mv new.ts ",
    "component-template": "cd templates/components; touch ",
    "directive-template": "cd templates/directives; touch ",
    "service-template": "cd templates/services; touch ",
    "pipe-template": "cd templates/pipes; touch ",
    "start": "concurrently \"tsc -w\" \"gulp\" \"lite-server\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
